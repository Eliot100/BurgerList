<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".my_page">

    <TextView
        android:id="@+id/textView"
        android:layout_width="174dp"
        android:layout_height="89dp"
        android:layout_marginTop="44dp"
        android:text="my page"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.86"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/userListButton"
        android:layout_width="156dp"
        android:layout_height="32dp"
        android:layout_marginTop="64dp"
        android:autoSizeTextType="uniform"
        android:text="My List"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.27"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/CreateRestButton"
        android:layout_width="158dp"
        android:layout_height="37dp"
        android:layout_marginTop="56dp"
        android:autoSizeTextType="uniform"
        android:text="create restaurant"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.217"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userRestButton" />

    <Button
        android:id="@+id/userRestButton"
        android:layout_width="159dp"
        android:layout_height="57dp"
        android:layout_marginTop="20dp"
        android:text="My Restaurant"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.23"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userPageButton" />


</androidx.constraintlayout.widget.ConstraintLayout>